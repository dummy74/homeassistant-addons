{
  "name": "vcontrold",
  "version": "0.0.2",
  "url": "https://github.com/dummy74/homeassistant-addons/tree/main/vcontrold",
  "slug": "vcontrold",
  "description": "vcontrold daemon",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "map": ["ssl","config:rw", "share:rw"],
  "devices": ["/dev/ttyUSB0:/dev/ttyUSB0:rwm","/dev/ttyAMA0:/dev/ttyAMA0:rwm"],
  "ports": {
    "3002/tcp": 3002
  },
  "options":
  {
    "mqtt_host": "hassio.local",
    "mqtt_user": "",
    "mqtt_password": "",
    "interval": 60,
    "vcommands": ["getTempA", "getTempWW"]
  },
  "schema":
  {
    "mqtt_host": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "interval": "int",
    "vcommands": "list"
   },
  "full_access": true
}
